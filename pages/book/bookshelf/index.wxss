.my-shelf-wrap{
  background: #FFF;
  width: 100%;
}
.my-shelf-wrap-inner{
    margin:20rpx 20rpx ;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-wrap: wrap;
}
.my-shelf-wrap-inner > view {
    background: red;
    height: 270rpx;
    width: 210rpx;
    line-height: 270rpx;
    text-align: center;
    margin:26rpx 10rpx;
    border-radius: 8rpx;
    box-shadow: 6rpx 4rpx 6rpx #aaa;
    border: 1px solid #eee;
    position: relative;
}

.recommend::before,.update::before{
    content:'';
    background: url('data:image/png;base64,');
    display: inline-block;
    width: 76rpx;
    height: 32rpx;
    background-size: 100% 100%;
    /* background: red; */
    position: absolute;
    left: 0;
    top: 10rpx;
}
.update::before{
    background: url('data:image/png;base64,');
    background-size: 100% 100%;
}

.prop-win{
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100rpx;
  background: #efefef;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  /* padding: 10rpx; */
}
.del-btn{
  margin-left: 20rpx;
}
.sel-btn{
  margin-right: 20rpx;
}

checkbox{
  font-size: 12px;
}